---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Site pro de daniel</title>
 <link href="http://danc.alwaysdata.net/atom.xml" rel="self"/>
 <link href="http://danc.alwaysdata.net/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://danc.alwaysdata.net/</id>
 <author>
   <name>Site pro de daniel</name>
 </author>

 {% for post in site.posts limit: 10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://danc.alwaysdata.net{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://danc.alwaysdata.net{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
